@model List<WebApplication5.Models.MaterialMasterList>

@{
    ViewBag.Title = "Expired Materials";
}

<h3 class="text-center">Expired Materials</h3>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>SNo</th>
            <th>Material Name</th>
            <th>Expiry Date</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>@(i + 1)</td>
                <td>@Model[i].MaterialSubCategory</td>
                <td>@string.Format("{0:dd-MM-yyyy}", Model[i].ExpiryDate)</td>

            </tr>
        }
    </tbody>
</table>
